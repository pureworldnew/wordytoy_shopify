}

@mixin style-rounded($directions, $border-radius) {
  @if $directions == all {
    border-radius: $border-radius;
  } @else if $directions == left {
    border-radius: $border-radius 0 0 $border-radius;
  } @else if $directions == right {
    border-radius: 0 $border-radius $border-radius 0;
  } @else if $directions == bottom {
    border-radius: 0 0 $border-radius $border-radius;
  } @else if $directions == bottom-left {
    border-radius: 0 0 0 $border-radius;
  } @else if $directions == bottom-right {
    border-radius: 0 0 $border-radius 0;
  }
}

@mixin style-pill($directions, $size) {
  @if $size == default {
    $size: $embellishment-pill-radius;
  } @else if $size == small {
    $size: $embellishment-pill-radius-small;
  }

  @include style-rounded($directions, $size);
}

@mixin style-soft-round($directions, $size) {
  @if $size == default {
    $size: $embellishment-soft-round-radius;
  } @else if $size == small {
    $size: $embellishment-soft-round-radius-small;
  }

  @include style-rounded($directions, $size);
}

@mixin style-cornered($directions, $size) {
  @if $size == default {
    $size: $embellishment-cornered;
  } @else if $size == small {
    $size: $embellishment-cornered-small;
  }

  @if $directions == left or $directions == bottom-left {
