$pattern-size-small: (rem(30px), rem(20px));
$pattern-size-large: (rem(60px), rem(40px));

// Z-indices
$z-index-image-zoom: 7000;
$z-index-product-poppers-desktop: 6000;
$z-index-product-poppers-mobile: 5000;
$z-index-poppers: 4500;
$z-index-action-bar: $z-index-poppers - 10;
$z-index-modal: 4000;
$z-index-announcement-bar: 3000;
$z-index-header-main: 3000;
$z-index-banners: 2000;
$z-index-content: 1000;
$z-index-footer: 500;

// Contextual z-index stacks
$z-index-header-inner: 1000;
$z-index-header-shadow: $z-index-header-inner - 100;

// Horizontal Spacing
$gutter-xs: rem($font-size-base) / 4; // 4px
$gutter-small: rem($font-size-base); // 16px
$gutter-medium: rem($font-size-base * 1.5); // 24px
$gutter-large: rem($font-size-base * 2); // 32px
$gutter-xl: rem($font-size-base * 3.5); // 56px

// Vertical Spacing
$space-xxx-small: rem($font-size-base * 0.25); // 4px
$space-xx-small: rem($font-size-base * 0.5); // 8px
$space-x-small: rem($font-size-base * 0.75); // 12px
$space-small: rem($font-size-base); // 16px
$space-medium: rem($font-size-base * 1.25); // 20px
$space-large: rem($font-size-base * 1.5); // 24px
$space-x-large: rem($font-size-base * 2); // 32px
$space-xx-large: rem($font-size-base * 2.5); // 40px
$space-xxx-large: rem($font-size-base * 3); // 48px

$space-page-small: $space-large;
$space-page-medium: $space-x-large;
$space-page-large: rem(70px);

// Grid spacing
$grid-space-x-small: $space-large;
$grid-space-small: $space-large * 2;
$grid-space-medium: rem($font-size-base * 4.25);
$grid-space-large: rem($font-size-base * 7);

// Layout
$max-width: rem(1216px) + ($gutter-large * 2);
$max-width-small: rem(500px);
$max-width-medium: rem(700px);

// Breakpoints
$bp-xs: 320px;
$bp-small: 376px;
$bp-medium: 769px;
$bp-large: 1025px;
$bp-xl: rem2px($max-width) + 1px;

// Header
$header-mobile-text: $color-button-primary-text;
$header-mobile-background: $color-primary;
$header-mobile-height: rem(60px);
$header-mobile-drawer-height: rem(320px);

$header-desktop-height: rem(56px);
$header-desktop-text: $color-secondary;
$header-desktop-text-hover: $color-secondary-hover;
$header-desktop-background: $color-background;

$header-icon-size: rem(24px);
$header-logo-width: rem(200px) + ($gutter-medium * 2);
$header-side-element-width: calc(50% - #{rem-round($header-logo-width / 2)});
$header-shadow: 0 2px 5px $color-shadow;

$header-mobile-icon-gutter: $gutter-small / 2;

// Form elements
$color-forms-background: $color-background-alternate;
$color-forms-background-alternate: scale-color($color-forms-background, $lightness: -5%);
$color-forms-text: scale-color(mix($color-primary, $color-text, 5%), $lightness: 10%);
$color-forms-border: desaturate(mix($color-background-alternate, $color-primary, 85%), 40%);
$color-forms-selected: $color-primary;
$color-text-fields-placeholder: scale-color($color-forms-text, $lightness: 20%);

$form-height: rem(48px);
$form-large-qty-width: $form-height;
$form-chevron-size: $font-size-body;
$form-checkbox-size: rem(20px);
$form-padding-h: $gutter-small;
$form-padding-v: rem(14px);
$form-min-width: rem(144px);

// Cards
$card-aspect-ratio: percentage(1);
$product-ratio-style: {{ settings.product_image_ratio }};

//$product-ratio-style is 'cropped-square'
$product-aspect-ratio: percentage(1);
@if $product-ratio-style == 'cropped-short' {
  $product-aspect-ratio: percentage(2 / 3);
} @else if $product-ratio-style == 'cropped-tall' {
  $product-aspect-ratio: percentage(3 / 2);
}

// Embellishments
$embellishment-icon: {{ settings.embellishment_icons }};
$embellishment-edges: {{ settings.embellishment_edges }};
$embellishment-pill-radius: rem(24px);
$embellishment-soft-round-radius: rem(5px);
$embellishment-cornered: rem(17px);

$embellishment-pill-radius-small: rem(8px);
$embellishment-soft-round-radius-small: rem(5px);
$embellishment-cornered-small: rem(9px);

$embellishment-edge-shadow: 0 2px 5px $color-shadow;
$embellishment-edge-border: $color-shadow;

// Social media colors
$color-facebook: #3b5998;
$color-twitter: #1da1f2;
$color-facebook-messenger: #007bff;
$color-whatsapp: #12d35b;

// Sections
$space-section-small: rem($font-size-base * 3);
$space-section-large: rem($font-size-base * 6);

$section-heading-color: $color-headings;
$section-subheading-color: $color-secondary;

@mixin media($max: false, $min: false) {
  @if $max {
    @if $min {
      @media screen and (max-width: $max - 1) and (min-width: $min) { @content; }
    } @else {
      @media screen and (max-width: $max - 1) { @content; }
    }
  } @else {
    @media screen and (min-width: $min) { @content; }
  }
}

@mixin dcb-narrow {
  @media screen and (min-width: 480px) and (max-width: #{$bp-medium - 1}),
    (min-width: 950px) {
    @content;
  }
}

%button-reset {
  @include font($font-buttons, $font-weight: bolder);
  display: inline-block;
  padding: 0;
  vertical-align: middle;
  cursor: pointer;
  background: transparent;
  border: 0;
  -webkit-appearance: normal;
     -moz-appearance: normal;
          appearance: normal;
}

%page-vertical-space {
  padding-bottom: $space-page-small;

  @include media($min: $bp-medium) {
    padding-bottom: $space-page-large;
  }
}

%masthead {
  @extend %layout-container-inner;
  padding-top: $space-page-small;
  padding-bottom: $space-page-small;
  text-align: center;

  @include media($min: $bp-medium) {
    padding-top: $space-page-medium;
    padding-bottom: $space-page-medium;
  }

  @include media($min: $bp-large) {
    padding-top: $space-page-large;
    padding-bottom: $space-page-large;
  }
}

%masthead-title {
  margin-top: 0;
  margin-bottom: 0;
  font-size: $font-size-large;
  line-height: 1.3;

  @include media($min: $bp-medium) {
    font-size: $font-size-x-large;
  }

  @include media($min: $bp-large) {
    font-size: $font-size-xx-large;
  }
}

%masthead-subtitle {
  @include font($font-heading, $font-weight: bolder);
  max-width: $max-width-medium;
  margin: 0 auto $space-xx-small;
  font-size: $font-size-smaller;
  color: $section-subheading-color;
  text-transform: uppercase;

  // Use %section-heading margin when is last child
  &:last-child {
    margin-bottom: $space-large;
  }

  @include media($min: $bp-medium) {

    &:last-child {
      margin-bottom: $space-xx-large;
    }
  }
}

%layout-gutters {
  padding-right: $gutter-small;
  padding-left: $gutter-small;

  @include media($min: $bp-medium) {
    padding-right: $gutter-medium;
    padding-left: $gutter-medium;
  }

  @include media($min: $bp-large) {
    padding-right: $gutter-large;
    padding-left: $gutter-large;
  }
}

%layout-container-inner {
  @extend %layout-gutters;
  max-width: $max-width;
  margin-right: auto;
  margin-left: auto;
}

%header-list-item {
  display: inline-block;

  &:not(:first-child) {
    margin-left: $gutter-small / 2;

    @include media($min: $bp-medium) {
      margin-left: $gutter-medium;
    }

    @include media($min: $bp-large) {
      margin-left: $gutter-large;
    }
  }
}

%header-list-item-link {
  @include font($font-header, $font-weight: bolder);
  font-size: $font-size-body;
  color: $header-desktop-text;

  &:hover {
    color: $header-desktop-text-hover;
  }
}

%icon-svg {
  display: block;
  width: auto;
  max-width: 100%;
  max-height: 100%;
  -webkit-transform-origin: 50% 50%;
      -ms-transform-origin: 50% 50%;
          transform-origin: 50% 50%;
  fill: currentColor;
}

%link-default {
  color: $color-link;
  text-decoration: none;
  -webkit-transition: color 0.2s;
  transition: color 0.2s;

  &:hover {
    color: $color-link-hover;
  }
}

%link-alt {
  color: $color-link-alt;
  text-decoration: none;
  -webkit-transition: color 0.2s;
  transition: color 0.2s;

  &:hover {
    color: $color-link-alt-hover;
  }
}

%link-tertiary {
  color: $color-link-tertiary;
  text-decoration: none;
  -webkit-transition: color 0.2s;
  transition: color 0.2s;

  &:hover {
    color: $color-link-tertiary-hover;
  }
}

$local-header-icon-dur: #{$transition-duration * 5/8};
$local-header-icon-dur-half: #{$transition-duration * 5/8 / 2};

%header-icon-container {
  position: relative;
  width: $header-icon-size;
  height: $header-icon-size;

  .site-header-icon-normal,
  .site-header-icon-alternate {
    @extend %icon-svg;
    width: rem(24px);
    height: rem(24px);
  }

  .site-header-icon-normal {
    -webkit-transform: scale(1);
        -ms-transform: scale(1);
            transform: scale(1);
    -webkit-transition: -webkit-transform $local-header-icon-dur-half $transition-timing $local-header-icon-dur-half;
    transition: -webkit-transform $local-header-icon-dur-half $transition-timing $local-header-icon-dur-half;
    transition: transform $local-header-icon-dur-half $transition-timing $local-header-icon-dur-half;
    transition: transform $local-header-icon-dur-half $transition-timing $local-header-icon-dur-half, -webkit-transform $local-header-icon-dur-half $transition-timing $local-header-icon-dur-half;
  }

  .site-header-icon-alternate {
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -1 * $header-icon-size / 2;
    margin-left: -1 * $header-icon-size / 2;
    -webkit-transform: scale(0);
        -ms-transform: scale(0);
            transform: scale(0);
    -webkit-transition: -webkit-transform $local-header-icon-dur $transition-timing;
    transition: -webkit-transform $local-header-icon-dur $transition-timing;
    transition: transform $local-header-icon-dur $transition-timing;
    transition: transform $local-header-icon-dur $transition-timing, -webkit-transform $local-header-icon-dur $transition-timing;
  }

  .has-active-toggle & {
    .site-header-icon-normal {
      -webkit-transform: scale(0);
          -ms-transform: scale(0);
              transform: scale(0);
      -webkit-transition: -webkit-transform $local-header-icon-dur $transition-timing;
      transition: -webkit-transform $local-header-icon-dur $transition-timing;
      transition: transform $local-header-icon-dur $transition-timing;
      transition: transform $local-header-icon-dur $transition-timing, -webkit-transform $local-header-icon-dur $transition-timing;
    }

    .site-header-icon-alternate {
      -webkit-transform: scale(1);
          -ms-transform: scale(1);
              transform: scale(1);
      -webkit-transition: -webkit-transform $local-header-icon-dur-half $transition-timing $local-header-icon-dur-half;
      transition: -webkit-transform $local-header-icon-dur-half $transition-timing $local-header-icon-dur-half;
      transition: transform $local-header-icon-dur-half $transition-timing $local-header-icon-dur-half;
      transition: transform $local-header-icon-dur-half $transition-timing $local-header-icon-dur-half, -webkit-transform $local-header-icon-dur-half $transition-timing $local-header-icon-dur-half;
    }
  }
}

%clearfix {
  &::before,
  &::after {
    display: table;
    content: "";
  }

  &::after {
    clear: both;
  }
}

