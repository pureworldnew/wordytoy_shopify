// Styles used through several areas, subjective to import ordering
.rte {
  $local-mobile-modifier: 0.7;
  font-size: $font-size-body;

  h1 {
    font-size: max($font-size-h1 * $local-mobile-modifier, $font-size-body);

    @include media($min: $bp-medium) {
      font-size: $font-size-h1;
    }
  }

  h2 {
    font-size: max($font-size-h2 * $local-mobile-modifier, $font-size-body);

    @include media($min: $bp-medium) {
      font-size: $font-size-h2;
    }
  }

  h3 {
    font-size: max($font-size-h3 * $local-mobile-modifier, $font-size-body);

    @include media($min: $bp-medium) {
      font-size: $font-size-h3;
    }
  }

  h4 {
    font-size: max($font-size-h4 * $local-mobile-modifier, $font-size-body);

    @include media($min: $bp-medium) {
      font-size: $font-size-h4;
    }
  }

  h5 {
    font-size: max($font-size-h5 * $local-mobile-modifier, $font-size-body);

    @include media($min: $bp-medium) {
      font-size: $font-size-h5;
    }
  }

  h6 {
    font-size: max($font-size-h6 * $local-mobile-modifier, $font-size-body);

    @include media($min: $bp-medium) {
      font-size: $font-size-h6;
    }
  }

  h1,
  h2,
  h3,
  h4,
  h5,
  h6 {
    margin-top: $space-xxx-large;
    margin-bottom: $space-small;
    line-height: 1.3;
    color: $color-headings;
  }

  dl,
  ul,
  ol,
  p,
  blockquote,
  img:not([style]) {
    margin-top: $font-size-body;
    margin-bottom: $font-size-body;
  }

  a {
    @extend %link-default;
  }

  blockquote {
    padding-left: $font-size-body;
    margin-left: 0;
    border-left: 2px solid $color-secondary;
  }

  ul,
  ol {
    padding-left: $gutter-small;
    list-style-position: inside;

    @include media($min: $bp-medium) {
      padding-left: $gutter-medium;
    }

    @include media($min: $bp-large) {
      padding-left: $gutter-large;
    }
  }

  dl {
    dt {
      margin-top: $font-size-body;
    }

    dd {
      margin-left: $gutter-small;

      @include media($min: $bp-medium) {
        margin-left: $gutter-medium;
      }

      @include media($min: $bp-large) {
        margin-left: $gutter-large;
      }
    }
  }

  img {
    max-width: 100%;
    margin-top: $font-size-body;
    margin-bottom: $font-size-body;

    &[style*="left"] {
      margin-top: 0;
      margin-right: $gutter-small;

      @include media($min: $bp-medium) {
        margin-right: $gutter-medium;
      }

      @include media($min: $bp-large) {
        margin-right: $gutter-large;
      }
    }

    &[style*="right"] {
      margin-top: 0;
      margin-left: $gutter-small;

      @include media($min: $bp-medium) {
        margin-left: $gutter-medium;
      }

      @include media($min: $bp-large) {
        margin-left: $gutter-large;
      }
    }
  }

  figure {
    margin-top: $space-small;
    margin-bottom: $space-small;
  }

  > *:first-child {
    margin-top: 0;
  }

  > *:last-child {
    margin-bottom: 0;
  }

  .tabs,
  .tabs-content {
    padding-left: 0;
    margin-left: 0;
    list-style: none;
  }

  .tabs {
    display: table;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: start;
    -webkit-align-items: flex-start;
        -ms-flex-align: start;
            align-items: flex-start;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
        -ms-flex-direction: row;
            flex-direction: row;
    -webkit-box-pack: start;
    -webkit-justify-content: flex-start;
        -ms-flex-pack: start;
            justify-content: flex-start;
    width: 100%;
    height: auto;

    li {
      @extend %link-alt;
      @include font($font-body, $font-weight: bolder, $font-family: false);
      display: table-cell;
      -webkit-box-flex: 0;
      -webkit-flex-grow: 0;
          -ms-flex-positive: 0;
              flex-grow: 0;
      -webkit-flex-shrink: 1;
          -ms-flex-negative: 1;
              flex-shrink: 1;
      vertical-align: top;
      cursor: pointer;
      outline-offset: rem(2px);
      outline-color: currentColor;

      &:not(:last-child) {
        margin-right: $gutter-small;
      }

      &.active {
        @extend %link-default;
      }

       a {
         color: currentColor;
       }
    }
  }

  .tabs-content {

    > li {
      display: none;

      &.active {
        display: block;
      }
    }
  }
}

@-webkit-keyframes shimmer {
  from {
    opacity: 0.5;
  }

  to {
    opacity: 1;
  }
}

@keyframes shimmer {
  from {
    opacity: 0.5;
  }

  to {
    opacity: 1;
  }
}

.placeholder-loading-text,
.placeholder-svg-loading {
  -webkit-animation: shimmer 0.7s ease-in-out infinite;
          animation: shimmer 0.7s ease-in-out infinite;
  -webkit-animation-fill-mode: forwards;
          animation-fill-mode: forwards;
  -webkit-animation-direction: alternate;
          animation-direction: alternate;
