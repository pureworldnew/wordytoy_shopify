@mixin edge-style-bordered($directions: all, $colors: $colors-default, $size: default) {
  @if $embellishment-edges == "pill" {
    @include bordered-edge-pill($directions, $colors, $size);
  }

  @if $embellishment-edges == "soft_round" {
    @include bordered-edge-soft-round($directions, $colors, $size);
  }

  @if $embellishment-edges == "cornered" {
    @include bordered-edge-cornered($directions, $colors, $size);
  }

  @if $embellishment-edges == "none" {
    @include bordered-edge-base($colors);
  }
}

@mixin edge-style-simple($directions: all, $size: default) {
  @if $embellishment-edges == "pill" {
    @include style-pill($directions, $size);
  }

  @if $embellishment-edges == "soft_round" {
    @include style-soft-round($directions, $size);
  }

  @if $embellishment-edges == "cornered" {
    @include style-cornered($directions, $size);
  }

  @if $embellishment-edges == "none" {
    border-radius: 0;
  }

  overflow: hidden;
}

@mixin edge-style-simple-small($directions: all, $size: default) {
  $local-size: rem(8px);

  // Pill and cornered both use 8px
  @if $embellishment-edges == "soft_round" {
    $local-size: rem(5px);
  }

  @include edge-style-simple($directions, $local-size)
}

@mixin edge-style-bordered-small($directions: all, $colors: $colors-default, $size: default) {
  $local-size: rem(8px);

  // Pill and cornered both use 8px
  @if $embellishment-edges == "soft_round" {
