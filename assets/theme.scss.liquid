@mixin bordered-edge-base($colors) {
  overflow: hidden;
  color: nth($colors, 1);
  background-color: nth($colors, 2);
  border: 1px solid $embellishment-edge-border;
  -webkit-box-shadow: $embellishment-edge-shadow;
