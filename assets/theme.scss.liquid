}

@mixin bordered-edge-pill($directions, $colors, $size) {
  @include bordered-edge-base($colors);
  @include style-pill($directions, $size);
}

@mixin bordered-edge-soft-round($directions, $colors, $size) {
  @include bordered-edge-base($colors);
  @include style-soft-round($directions, $size);
}

@mixin bordered-edge-cornered($directions, $colors, $size) {
  @include bordered-edge-base($colors);
  @include style-cornered($directions, $size);
  -webkit-box-shadow: none;
          box-shadow: none;

  @supports ((-webkit-clip-path: polygon(0 0)) or (clip-path: polygon(0 0))) or (-webkit-clip-path: polygon(0 0)) {
    &::before {
      position: absolute;
      top: -1px;
      left: -1px;
      z-index: 1;
      width: calc(100% + 2px);
      height: calc(100% + 2px);
      pointer-events: none;
      background-color: $embellishment-edge-border;
      content: "";
    }
  }
}

/*
@include edge-style-bordered() and @include edge-style-simple()
  Apply edge style to component
  - By default, edge style is applied to all corners
  - Cornered style
    - Always applies to bottom edges
  - Pill and Soft Round
      - left (top and bottom edges)
      - right (top and bottom edges)
      - bottom-left (bottom left edge only)
      - bottom-right (bottom right edge only)
